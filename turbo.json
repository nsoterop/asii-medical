{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "dev": {
      "cache": false,
      "persistent": true
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "dist/**", "coverage/**"]
    },
    "lint": {
      "outputs": []
    },
    "format": {
      "outputs": []
    },
    "format:check": {
      "outputs": []
    },
    "typecheck": {
      "outputs": []
    },
    "test:unit": {
      "outputs": []
    },
    "test:e2e": {
      "outputs": []
    },
    "env:check": {
      "outputs": []
    },
    "clean": {
      "cache": false,
      "outputs": []
    }
  }
}
