# Core
API_PORT=3001
DATABASE_URL=postgresql://user:password@localhost:5432/postgres
ADMIN_SHARED_SECRET=
REDIS_URL=redis://localhost:6379

# Supabase (dev: local, prod: hosted)
SUPABASE_URL=
# Derived automatically from SUPABASE_URL if omitted.
SUPABASE_JWKS_URL=
SUPABASE_ISSUER=
SUPABASE_AUDIENCE=authenticated
# Optional: required for local JWT verification when Supabase uses HS256.
SUPABASE_JWT_SECRET=
SUPABASE_SERVICE_ROLE_KEY=

# Search
MEILI_URL=http://localhost:7700
MEILI_MASTER_KEY=

# Imports
IMPORT_BATCH_SIZE=500
IMPORT_CONCURRENCY=4
IMPORT_RUN_STUCK_MINUTES=30
IMPORT_WORKER_ENABLED=true

# Payments (set to mock to skip Square secrets in dev)
PAYMENTS_PROVIDER=mock

# Square
SQUARE_ENV=sandbox
SQUARE_ACCESS_TOKEN=
SQUARE_LOCATION_ID=
SQUARE_WEBHOOK_SIGNATURE_KEY=
SQUARE_APP_ID=
SQUARE_CURRENCY=USD

# Tax
TAX_PROVIDER=none
TAX_STATE_RATES=
TAX_ORIGIN_COUNTRY=US
TAX_ORIGIN_STATE=
TAX_ORIGIN_ZIP=
TAX_ORIGIN_CITY=
TAX_ORIGIN_STREET=

# Email
EMAIL_PROVIDER=log
EMAIL_FROM=
RESEND_API_KEY=
